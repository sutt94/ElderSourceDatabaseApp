@model IEnumerable<ElderSourceApp.Models.CompanyListViewModel>
<div id="companyList">

    <table class="table">
        <tr>
            <th>
                Name
            </th>

            <th>
                City
            </th>
            <th>
                State
            </th>
            <th>
                Zip Code
            </th>
            <th>
                Company Type
            </th>
            <th>
                Phone Number
            </th>
            <th></th>
        </tr>



        @foreach (var item in Model)
            {



            <tr>
                <td>
                    @item.CompanyName
                </td>
                <td>
                    @item.City
                </td>
                <td>
                    @item.State
                </td>
                <td>
                    @item.ZipCode
                </td>
                <td>
                    @item.CompanyType
                </td>
                <td>
                    @item.Phone
                </td>
                <td>
                    @Html.ActionLink("Details", "Details", new { id = item.CompanyModelID })
                </td>



            </tr>




        }
    </table>

    

</div>

